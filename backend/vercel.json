{
  "version": 2,
  "builds": [
    {
      "src": "/frontend/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/(.*)",
      "dest": "/backend/src/server.ts"
    }
  ]
}
